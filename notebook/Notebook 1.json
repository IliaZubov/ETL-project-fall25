					"import json\n",
					"import pandas as pd\n",
					"\n",
					"url = \"https://api.spacexdata.com/v3/launches\"\n",
					"headers = {\n",
					"    \"Content-Type\": \"application/json\"\n",
					"}\n",
					"\n",
					"response = requests.get(url)\n",
					"data = response.json()"
				],
				"execution_count": 69
			},
			{
				"cell_type": "code",
				"source": [
					"data"
				],
				"execution_count": 70
			},
			{
				"cell_type": "code",
				"source": [
					"df_pandas = pd.json_normalize(\n",
					"    data,  # your list of dicts\n",
					"    meta=[\n",
					"        'flight_number',\n",
					"        'mission_name',\n",
					"        'launch_year',\n",
					"        'launch_date_utc',\n",
					"        'launch_success',\n",
					"        ['rocket', 'rocket_id'],\n",
					"        ['rocket', 'rocket_name'],\n",
					"        ['rocket', 'rocket_type'],\n",
					"        ['launch_site', 'site_name'],\n",
					"        ['launch_failure_details', 'reason']\n",
					"    ],\n",
					"    record_path=['rocket', 'second_stage', 'payloads'],  # nested array to flatten\n",
					"    errors='ignore'\n",
					")"
				],
				"execution_count": 75
			},
			{
				"cell_type": "code",
				"source": [
					"df_pandas.columns = [col.replace('.', '_').replace('[', '').replace(']', '') for col in df_pandas.columns]"
